{
  "family": "hello-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::aws_accontno:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hello",
      "image": "aws_accontno.dkr.ecr.us-east-1.amazonaws.com/hello-ts:v1",
      "portMappings": [
        { "containerPort": 3000, "protocol": "tcp" }
      ],
      "essential": true
    }
  ]
}
